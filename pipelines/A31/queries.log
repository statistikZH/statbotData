Query Nr. 12
-- In welchem Jahr war das durchschnittliche steuerbare Einkommen in Basel-Stadt am höchsten?
SELECT T.jahr FROM basel_stadt_steuerstatistik_kennzahlen_wohnvierteln AS T JOIN spatial_unit AS S ON T.spatialunit_uid = S.spatialunit_uid WHERE S.residence_area = TRUE GROUP BY T.jahr ORDER BY (SUM(T.anzahl_veranlagungen * T.steuerbares_einkommen_mittelwert)) / SUM(T.anzahl_veranlagungen) DESC LIMIT 1;
