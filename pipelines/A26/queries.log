Query Nr. 1
-- Wie hoch waren die Versicherungssummen für Brandschäden im Aargau im Jahr 2017 insgesamt in CHF?
SELECT T.total_schaden_1000_chf * 1000 AS total_schaden_chf FROM aargau_brandversicherung_brandschaden_anzahl_schadensummen AS T JOIN spatial_unit AS S ON T.spatialunit_uid = S.spatialunit_uid WHERE S.canton = TRUE     AND S.name LIKE '%Aargau%'     AND T.jahr = 2017;
