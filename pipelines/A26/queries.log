Query Nr. 7
-- In Prozent, wie hat sich die Zahl der Elektrobrände im Kanton Aargau von 1990 bis 2020 verändert?
SELECT 100.0 * (T1.anzahl_2020 - T1.anzahl_1990) / T1.anzahl_1990 AS prozent_anderung_elektrizitaet_anzahl_schadenfalle_1990_2020 FROM (     SELECT         SUM(CASE WHEN T.jahr = 1990 THEN T.elektrizitaet_anzahl_schadenfalle ELSE 0 END) AS anzahl_1990,         SUM(CASE WHEN T.jahr = 2020 THEN T.elektrizitaet_anzahl_schadenfalle ELSE 0 END) AS anzahl_2020     FROM aargau_brandversicherung_brandschaden_anzahl_schadensummen AS T     JOIN spatial_unit AS S ON T.spatialunit_uid = S.spatialunit_uid     WHERE S.canton = TRUE         AND S.name LIKE '%Aargau%'         AND T.jahr IN (1990, 2020) ) AS T1;
