Query Nr. 1
-- Welches waren die fünf am meisten importierten Energietraeger 2020 in der Schweiz und wieviel wurde importiert?
SELECT nutzungs_sektor, energietraeger, SUM(energiemenge_in_tera_joule) as tj_import_2020 FROM energiebilanz_schweiz_in_tera_joule WHERE jahr='2020' AND LOWER(nutzungs_sektor) LIKE '%import%' AND energiemenge_in_tera_joule IS NOT NULL GROUP BY energietraeger, nutzungs_sektor ORDER BY tj_import_2020 DESC;
