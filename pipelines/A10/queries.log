Query Nr. 4
-- Welches war die am meisten begangene Straftat 2020 im Kanton Zürich?
SELECT T.straftat as haufigste_straftat_2020_kanton_zurich FROM straftaten_aufklaerung as T JOIN spatial_unit as S on T.spatialunit_uid = S.spatialunit_uid WHERE S.name_de = 'Schweiz' AND S.country = TRUE AND T.ausfuhrungsgrad = 'Vollendet' AND T.aufklarungsgrad  = 'Aufklärungsgrad - Total' GROUP BY straftat ORDER BY anzahl_straftaten LIMIT 1;
