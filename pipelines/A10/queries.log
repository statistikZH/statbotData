Query Nr. 4
-- Welches war die am meisten begangene Straftat 2020 im Kanton Bern?
SELECT T.straftat, S.name_de, T.jahr, T.anzahl_straftaten FROM straftaten_aufklaerung as T JOIN spatial_unit as S on T.spatialunit_uid = S.spatialunit_uid WHERE S.name_de LIKE '%Bern%' AND S.canton = TRUE AND T.jahr=2020 AND T.straftat<>'Straftat - Total' AND T.anzahl_straftaten IS NOT NULL GROUP BY T.straftat, S.name_de, T.jahr, T.anzahl_straftaten ORDER BY T.anzahl_straftaten DESC LIMIT 1;
