Query Nr. 1
-- In welchem Jahr gab es die meisten Straftaten in der Schweiz?
SELECT T.jahr, S.name_de, T.anzahl_straftaten FROM straftaten_aufklaerung as T JOIN spatial_unit as S on T.spatialunit_uid=S.spatialunit_uid WHERE S.name_de='Schweiz' AND S.country=TRUE AND T.straftat='Straftat - Total' GROUP BY T.jahr, T.anzahl_straftaten, S.name_de ORDER BY T.anzahl_straftaten DESC LIMIT 1;
