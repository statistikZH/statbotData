Query Nr. 6
-- Zeigen Sie mir die Entwicklung der Bevölkerung von Pratteln, in Basel-Landschaft, zwischen 2017 und 2019.
SELECT T.jahr, SUM(T.gesamt_anzahl_personen) as population FROM basel_land_bevolkerung_nach_nationalitat_konfession_gemeinde as T JOIN spatial_unit AS S ON T.spatialunit_uid = S.spatialunit_uid WHERE S.municipal=TRUE     AND S.name IN ('Pratteln', 'Pratteln (BL)')     AND T.jahr>=2017     AND T.jahr<=2019 GROUP BY T.jahr ORDER BY T.jahr ASC;
