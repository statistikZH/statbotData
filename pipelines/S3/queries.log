
Query Nr. 1
-- Welche Teilung hatte im Jahr 2020 die höchsten Staatsausgaben?
SELECT aufgabenbereich_cofog_teilung as ausgaben_bereich_mit_den_hoechsten_ausgaben_2020 FROM staatsausgaben_nach_aufgabenbereichen_cofog WHERE institutioneller_sektor="Staat" AND jahr=2020 GROUP BY aufgabenbereich_cofog_teilung ORDER BY SUM(ausgaben_in_mio_chf) DESC LIMIT 1;
 ausgaben_bereich_mit_den_hoechsten_ausgaben_2020
                                     Sozialschutz

Query Nr. 2
-- Welches waren die 3 grössten Ausgabenquellen des Bundes im Jahr 2021 und wie hoch waren diese?
SELECT SUM(ausgaben_in_mio_chf),aufgabenbereich_cofog_gruppen as ausgaben_bereich_mit_den_hoechsten_ausgaben_2020 FROM staatsausgaben_nach_aufgabenbereichen_cofog WHERE institutioneller_sektor="Bund" AND jahr=2020 GROUP BY aufgabenbereich_cofog_gruppen ORDER BY SUM(ausgaben_in_mio_chf) DESC LIMIT 3;
