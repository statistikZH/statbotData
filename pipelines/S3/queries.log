Query Nr. 4
-- Wie hoch waren die religionsbezogenen Ausgaben der Kantone im Jahr ihres Höchststandes?
SELECT t1.jahr, t1.aufgabenbereich_cofog_feingliederung, t1.ausgaben_in_mio_chf FROM staatsausgaben_nach_aufgabenbereichen_cofog t1 INNER JOIN (     SELECT jahr, SUM(ausgaben_in_mio_chf) AS tot_spending     FROM staatsausgaben_nach_aufgabenbereichen_cofog     WHERE LOWER(aufgabenbereich_cofog_feingliederung) LIKE '%religion%'       AND institutioneller_sektor = 'Kantone'     GROUP BY jahr     ORDER BY tot_spending DESC LIMIT 1 ) t2 ON t1.jahr = t2.jahr WHERE LOWER(aufgabenbereich_cofog_feingliederung) LIKE '%religion%'   AND t1.institutioneller_sektor = 'Kantone';
