Query Nr. 8
-- Welcher Kanton kann nach den neuesten Informationen den günstigsten Strom für die Kategorie H3 anbieten?
SELECT S.name FROM median_strompreis_per_kanton as T JOIN spatial_unit as S on T.spatialunit_uid = S.spatialunit_uid WHERE S.canton=TRUE     AND T.jahr=(SELECT MAX(jahr) FROM median_strompreis_per_kanton)     AND T.verbrauchskategorie='H3' GROUP BY T.jahr, S.name, T.mittlerer_preis_rappen_pro_kwh ORDER BY T.mittlerer_preis_rappen_pro_kwh ASC LIMIT 1;
