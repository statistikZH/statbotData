Query Nr. 8
-- Welcher Kanton kann nach den neuesten Informationen den günstigsten Strom für die Kategorie H3 anbieten?
SELECT S.name FROM median_strompreis_per_kanton as T JOIN spatial_unit as S on T.spatialunit_uid = S.spatialunit_uid WHERE S.canton=TRUE     AND T.jahr=(SELECT MAX(jahr) FROM median_strompreis_per_kanton)     AND T.verbrauchskategorie="H3" ORDER BY T.mittlerer_preis_rappen_pro_kwh ASC LIMIT 1;
                name
 Canton of Nidwalden

Query Nr. 9
-- Wie hoch waren die Preise für das günstigste verfügbare Stromprodukt für alle Unternehmenskategorien im Kanton Genf im Jahr 2012?
SELECT T.verbrauchskategorie, MIN(T.mittlerer_preis_rappen_pro_kwh) FROM median_strompreis_per_kanton as T JOIN spatial_unit as S on T.spatialunit_uid = S.spatialunit_uid WHERE S.name_de LIKE '%Genf%'     AND S.canton=TRUE     AND T.jahr=2012     AND T.verbrauchskategorie LIKE '%C_%' GROUP BY T.verbrauchskategorie ORDER BY T.mittlerer_preis_rappen_pro_kwh ASC;
