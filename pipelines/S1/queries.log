Query Nr. 4
-- Welches ist der Kanton mit dem höchsten Preis für 1000 kWh im Jahr 2021 für das Standardstromprodukt der Kategorie C3?
SELECT S.name_de as kanton, T.jahr, T.verbrauchskategorie, T.energieprodukt FROM median_strompreis_per_kanton as T JOIN spatial_unit as S on T.spatialunit_uid = S.spatialunit_uid WHERE S.canton=TRUE     AND T.jahr=2021     AND T.energieprodukt='Standardprodukt'     AND T.verbrauchskategorie='C3' GROUP BY S.name_de, T.jahr, T.verbrauchskategorie, T.energieprodukt, T.mittlerer_preis_rappen_pro_kwh ORDER BY T.mittlerer_preis_rappen_pro_kwh DESC LIMIT 1;
