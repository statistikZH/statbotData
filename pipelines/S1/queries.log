Query Nr. 7
-- Wie haben sich die Standardstrompreise für kleine Unternehmen im Kanton Neuenburg zwischen 2017 und 2023 entwickelt?
SELECT T.jahr, T.energieprodukt, T.verbrauchskategorie, T.mittlerer_preis_rappen_pro_kwh FROM median_strompreis_per_kanton as T JOIN spatial_unit as S on T.spatialunit_uid = S.spatialunit_uid WHERE S.name_de LIKE '%Neuenburg%'     AND S.canton=TRUE     AND T.jahr >= 2017     AND T.jahr <= 2023     AND T.energieprodukt="Standardprodukt"     AND T.verbrauchskategorie_beschreibung LIKE '%Kleinbetrieb%' ORDER BY T.jahr;
